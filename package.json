{
    "name": "groscan",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "next build",
        "dev": "NODE_ENV=development next dev",
        "lint": "next lint",
        "start": "NODE_ENV=production next start",
        "postinstall": "prisma generate",
        "export": "next build && next export",
        "cap:sync": "npx cap sync android",
        "cap:copy": "npx cap copy android",
        "cap:run": "npx cap run android",
        "cap:copy-assets": "cp src/helpers/background.js out/background.js && cp assets/groscan_notification.wav out/groscan_notification.wav",
        "debug:android": "yarn export && yarn cap:copy-assets && yarn cap:sync && yarn cap:run",
        "vercel-build": "prisma generate && next build",
        "migrate:deploy": "prisma migrate deploy",
        "migrate:prisma": "prisma migrate dev",
        "dbpush:prisma": "prisma db push",
        "generate:prisma": "prisma generate",
        "introspect:prisma": "prisma introspect",
        "studio:prisma": "prisma studio",
        "format:prisma": "prisma format",
        "seed:prisma": "prisma db seed",
        "ai:dev": "python3 ai/manage.py runserver"
    },
    "prisma": {
        "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/server/db/seed.ts"
    },
    "dependencies": {
        "@apollo/client": "^3.9.11",
        "@apollo/server": "^4.10.3",
        "@as-integrations/next": "^3.0.0",
        "@capacitor/android": "^6.0.0",
        "@capacitor/background-runner": "^1.0.5",
        "@capacitor/core": "^6.0.0",
        "@capacitor/ios": "^6.0.0",
        "@capacitor/local-notifications": "^6.0.0",
        "@capacitor/preferences": "^6.0.0",
        "@next-auth/prisma-adapter": "^1.0.7",
        "@prisma/client": "^5.12.1",
        "@t3-oss/env-nextjs": "^0.9.2",
        "cloudinary": "^2.1.0",
        "cors": "^2.8.5",
        "graphql": "^16.8.1",
        "next": "^14.2.2",
        "next-auth": "^4.24.7",
        "nextjs-progressbar": "^0.0.16",
        "nexus": "^1.3.0",
        "nodemailer": "^6.9.13",
        "otp-generator": "^4.0.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-use-is-online": "^1.3.0",
        "winston": "^3.13.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@capacitor/assets": "^3.0.5",
        "@capacitor/cli": "^6.0.0",
        "@types/eslint": "^8.56.9",
        "@types/next-auth": "^3.15.0",
        "@types/node": "^20.12.7",
        "@types/nodemailer": "^6.4.14",
        "@types/otp-generator": "^4.0.2",
        "@types/react": "^18.2.79",
        "@types/react-dom": "^18.2.25",
        "@typescript-eslint/eslint-plugin": "^7.7.0",
        "@typescript-eslint/parser": "^7.7.0",
        "eslint": "^9.0.0",
        "eslint-config-next": "^14.2.2",
        "eslint-plugin-react": "^7.34.1",
        "prettier": "^3.2.5",
        "prisma": "^5.12.1",
        "pwa-asset-generator": "^6.3.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5"
    },
    "ct3aMetadata": {
        "initVersion": "7.22.0"
    },
    "packageManager": "yarn@1.22.19"
}
