{
    "name": "groscan",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "next build",
        "dev": "next dev",
        "lint": "next lint",
        "start": "next start",
        "postinstall": "prisma generate",
        "export": "next build && next export",
        "cap:sync": "npx cap sync android",
        "cap:copy": "npx cap copy android",
        "cap:run": "npx cap run android",
        "cap:copy-assets": "cp src/helpers/background.js out/background.js && cp assets/groscan_notification.wav out/groscan_notification.wav",
        "debug:android": "yarn export && yarn cap:copy-assets && yarn cap:sync && yarn cap:run",
        "vercel-build": "prisma generate && next build",
        "migrate:deploy": "prisma migrate deploy",
        "migrate:prisma": "prisma migrate dev",
        "dbpush:prisma": "prisma db push",
        "generate:prisma": "prisma generate",
        "introspect:prisma": "prisma introspect",
        "studio:prisma": "prisma studio",
        "format:prisma": "prisma format",
        "seed:prisma": "prisma db seed"
    },
    "prisma": {
        "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/server/db/seed.ts"
    },
